{
  "name": "cognivirues-outbreak-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "run:dev": "set NODE_ENV=development && nodemon src/index.ts",
    "run:dev:ts": "set NODE_ENV=development && ts-node src/index.ts",
    "start:dev": "set NODE_ENV=development && node dist/index.js",
    "start:prod": "set NODE_ENV=production && node dist/index.js",
    "prisma:format": "dotenv -e .env.development -- npx prisma format",
    "prisma:generate": "dotenv -e .env.development -- npx prisma generate",
    "prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:push": "dotenv -e .env.development -- npx prisma db push",
    "prisma:studio": "dotenv -e .env.development -- npx prisma studio",
    "test": "jest --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^5.20.0",
    "@types/jest": "^29.5.13",
    "@types/supertest": "^6.0.2",
    "bcrypt": "^5.1.1",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lodash.merge": "^4.6.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "openai": "^4.83.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.4",
    "nodemon": "^3.1.7",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
