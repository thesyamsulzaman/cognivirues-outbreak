{
  "name": "cognivirues-outbreak-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json --skipLibCheck",
    "test": "jest --verbose",
    "dev:run": "dotenv -e .env.development -- cross-env NODE_ENV=development nodemon --exec ts-node --transpile-only src/index.ts --ignore messages-db.json",
    "dev:run:ts": "dotenv -e .env.development -- cross-env NODE_ENV=development ts-node src/index.ts",
    "dev:start": "dotenv -e .env.development -- cross-env NODE_ENV=development node dist/index.js",
    "dev:prisma:format": "dotenv -e .env.development -- npx prisma format",
    "dev:prisma:generate": "dotenv -e .env.development -- npx prisma generate",
    "dev:prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "dev:prisma:push": "dotenv -e .env.development -- npx prisma db push --accept-data-loss",
    "dev:prisma:studio": "dotenv -e .env.development -- npx prisma studio",
    "prod:start": "dotenv -e .env.production -- cross-env NODE_ENV=production node dist/index.js",
    "prod:prisma:generate": "dotenv -e .env.production -- npx prisma generate",
    "prod:prisma:push": "dotenv -e .env.production -- npx prisma db push --accept-data-loss",
    "prod:prisma:studio": "dotenv -e .env.production -- npx prisma studio"
  },
  "prisma": {
    "seed": "ts-node prisma/seeds/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/jest": "^29.5.13",
    "@types/supertest": "^6.0.2",
    "bcrypt": "^5.1.1",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lodash.merge": "^4.6.2",
    "morgan": "^1.10.0",
    "openai": "^4.83.0",
    "pgvector": "^0.2.0",
    "prisma-extension-pgvector": "^0.11.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
